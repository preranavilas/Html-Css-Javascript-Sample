    top: 2em;
    width: 50%;
    z-index: 1001;
}

.credit-not-eligible-modal:before {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #fff;
    border-color: #a9acab;
    border-image: none;
    border-style: solid;
    border-width: 1px 0 0 1px;
    content: " ";
    height: 15px;
    position: absolute;
    right: 50%;
    top: -9px;
    transform: rotate(45deg);
    width: 15px;
}

.credit-not-eligible-modal.modal-open {
    display: block;
}

.credit-not-eligible-modal.modal-open + .credit-not-eligible-modal-mask {
    background: none repeat scroll 0 0 #333;
    bottom: 0;
    left: 0;
    opacity: 0.5;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 1000;
}

.credit-not-eligible-modal .credit-not-eligible-title {
    font-size: 10px;
    font-weight: bold;
    line-height: 20px;
    margin: 20px 0 12px;
    text-transform: uppercase;
}

.credit-not-eligible-modal button.credit-not-eligible-close {
    background: none repeat scroll 0 0 #39d2c9;
    border: medium none;
    color: #fff;
    cursor: pointer;
    display: inline-block;
    font-family: inherit;
    font-size: 14px;
    font-weight: normal;
    margin: 10px 0 0;
    padding: 20px 10px;
    text-align: center;
    text-transform: uppercase;
    width: 100%;
}

.credit-not-eligible-modal p {
    color: #585a59;
    font-family: inherit;
    font-size: 26px;
    font-weight: 200;
    line-height: 1.3;
    margin: 7px auto;
}

.credit-not-eligible-modal p a {
    color: #39d2c9 !important;
    display: block;
    font-family: inherit;
    margin-top: 2px;
}

.credit-not-eligible-modal p a:before {
    content: "c";
    font-family: "styleseatregular";
    margin-right: 14px;
    text-transform: none;
    vertical-align: text-top;
}

.form_error_text_for_attribution_new_client_answer_error, .form_error_text_for_attribution_client_survey_source_error {
    color: red;
}

#for_attribution_new_client_answer_error, #for_attribution_client_survey_source_error {
    float: left;
    margin-left: 39px;
    padding: 0;
}


