        padding: 30px 10px 20px;
        height: 811px
    }

    #tab-1.help-tab .tab-content {
        height: auto;
        padding: 10px 0;
        margin-bottom: 20px;
    }

    #tab-1.help-tab .tab-content-2 {
        height: 0;
        padding: 10px 0;
        margin-bottom: 20px;
    }

    .help-ic {
        border-radius: 100%;
        box-shadow: 3px 2px 4px #ddd;
        float: left;
        margin: -21px 14px 0 -8px;
        width: 60px;
        height: 59px;
