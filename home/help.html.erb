        padding: 40px 0 25px 0;
    }

    .help-tab-praph {
        line-height: 28px
    }

    .help-tab-praph a {
        color: #999;
        cursor: pointer;
    }

    .help-tab-praph a:hover {
        text-decoration: underline;
    }

    .tab-in-box {
        min-height: 120px;
        margin-left: 10px;
    }

    .help-heading {
        padding-left: 0;
        font-family: 'proxima_nova_rgregular';
        font-weight: 100;
        font-size: 17px;
        letter-spacing: 0
    }

