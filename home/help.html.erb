        background: #ca2a5c !important;
        color: #fff;
    }

    textarea {
        font-family: 'proxima_novalight';
    }

    a:hover, h4, label {
        font-family: 'proxima_nova_rgregular';
    }

    .tab-content-box input[type="checkbox"] + label {
        margin-right: 25px
    }

    .tab-content-box label {
        font-weight: 100
    }

    .tab-content-box h4 {
        font-size: 17px
    }

    .tab-content-box .btn-default {
        float: right;
        margin-right: 15px
    }

    .tab-content-box.contact {
        padding: 0 10px
    }

    .contFrm {
